# Upstream configuration for uWSGI
upstream uwsgi_app {
    server unix:/code/Metamarket/uwsgi_app.sock;
}

server {
    listen 80;
    server_name www.Metamarketproject.com Metamarketproject.com;

    # Logging configuration
    error_log stderr warn;
    access_log /dev/stdout main;

    # Location for dynamic requests
    location / {
        include /etc/nginx/uwsgi_params;
        uwsgi_pass uwsgi_app;
    }
}
